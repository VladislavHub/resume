<main class="g-5">
    <hr>

    <div>
        <h1>Web</h1>
    </div>

    <div>
        <h2 class="mb-3">Languages:</h2>
    </div>

    {{#with web}}

    <div>
        <ul>
            <li>Name: {{languages.[0].name}}</li>
