                <ul>
                    <li>Value: {{languages.[1].properties.[0].values.[0].value}}</li>
                    <li>Description: {{languages.[1].properties.[0].values.[0].description}}</li>

                    <li>Value: {{languages.[1].properties.[0].values.[1].value}}</li>
                    <li>Description: {{languages.[1].properties.[0].values.[1].description}}</li>
                </ul>

                <li class="mt-3">Name: {{languages.[1].properties.[1].name}}</li>
