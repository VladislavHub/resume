<main class="g-5">
    <div class="row">

        <div class="card px-0">
            <div class="card-body nav">
                {{#each navigation.links}}
                <div class="nav-item">
                    <a href="{{href}}" class="nav-link">{{text}}</a>
                </div>
                {{/each}}
            </div>
        </div>


        <div class="my-5">
            <ol class="breadcrumb">
                {{#each breadcrumb}}
                <li class="breadcrumb-item">
                    <a href="{{url}}">{{name}}</a>
                </li>
                {{/each}}
            </ol>
        </div>

        <div>
            <h1>{{header}}</h1>
        </div>



        <div class="row">

            {{#with sortBlock.[0]}}
            <div class="col-3">
                <label clas="form-label">{{label}}</label>
                <select class="form-select">
                    <option selected>{{placeholder}}</option>

                    {{#each options}}
                    <option value="{{value}}">
                        {{text}}</option>
                    {{/each}}

                </select>
            </div>
            {{/with}}

            {{#with sortBlock.[1]}}
            <div class="col-3">
                <label clas="form-label">{{label}}</label>
                <select class="form-select">
                    <option selected>{{placeholder}}</option>

                    {{#each options}}
                    <option value="{{value}}">
                        {{text}}</option>
                    {{/each}}

                </select>
            </div>
            {{/with}}

            {{#with sortBlock.[2]}}
            <div class="col-3">
                <label clas="form-label">{{label}}</label>
                <select class="form-select">
                    <option selected>{{placeholder}}</option>

                    {{#each options}}
                    <option value="{{value}}">
                        {{text}}</option>
                    {{/each}}

                </select>
            </div>
            {{/with}}

        </div>

        <div class="col-4 mt-5">
            {{!-- Left Block --}}
            <div class="card">
                <div class="card-body">
                    {{#with filterBlock}}
                    {{#with sellers}}
                    <div>
                        <div class="form-label">{{title}}</div>

                        <div class="form-check">
                            <label class="form-check-label">{{list.[0].value}}</label>
                            <input type="checkbox" class="form-check-input">
                        </div>

                        <div class="form-check">
                            <label class="form-check-label">{{list.[1].value}}</label>
                            <input type="checkbox" class="form-check-input">
                        </div>
                    </div>
                    {{/with}}

                    <hr>
                    {{#with brands}}
                    <div>
                        <label class="form-label">{{title}}</label>
                        <input placeholder="{{input.placeholder}}" class="form-control">
                    </div>

                    <div class="mt-3">

                        {{#each options}}
                        <div class="form-check">
                            <vabel class="form-check-label">{{value}}</vabel>
                            <input class="form-check-input" type="checkbox">
                        </div>
                        {{/each}}
                    </div>
                    {{/with}}

                    <hr>

                    {{#with price}}
                    <div>
                        <label class="form-label">{{title}}</label>
                        <input class="form-control" placeholder="{{range.from}}">
                    </div>

                    <div class="mt-2">
                        <input class="form-control" placeholder="{{range.to}}">
                    </div>

                    <div class="mt-3">
                        <a href="{{button.link}}" class="btn btn-outline-secondary">{{button.text}}</a>
                    </div>
                    {{/with}}

                    <hr>

                    {{#each filterList}}
                    <label class="form-label">{{title}}</label>
                    <div class="form-check">
                        {{#each options}}
                        <input class="form-check-input" type="checkbox">
                        <label class="form-check-label">{{value}}</label>
                        {{/each}}
                    </div>
                    <hr>
                    {{/each}}




