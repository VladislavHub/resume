            <div class="card-body">
                <h2 class="text-center">{{title}}</h2>
                <p class="text-center">{{description}}</p>

                <div class="text-center">
                    {{#each buttons}}
                    <a href="{{link}}" class="btn btn-primary text-center">{{text}}</a>
                    {{/each}}

                </div>
            </div>
        </div>
        {{/with}}

        <hr class="my-5">


        <footer>

            <div class="row">
                {{#each footer.[0]}}
                <div class="col-3 px-1">
                    <form action="{{link}}" target="_blank">
                        <button class="btn btn-link">{{text}}</button>
                    </form>
                </div>
                {{/each}}
            </div>

